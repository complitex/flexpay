<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC
		"-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN"
		"http://www.sun.com/software/dtd/appserver/sun-web-app_2_5-0.dtd">
<sun-web-app>

	<security-role-mapping>
		<role-name>ROLE_BASIC</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- ab role mappings for filter initialization -->
	<security-role-mapping>
		<role-name>ROLE_AB_COUNTRY_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_REGION_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_TOWN_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_DISTRICT_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_STREET_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_BUILDING_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<security-role-mapping>
		<role-name>ROLE_AB_APARTMENT_READ</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- operation roles mapping -->
	<security-role-mapping>
		<role-name>ROLE_PAYMENTS_OPERATION_READ</role-name>
		<group-name>PAYMENTS_FULL_ACCESS</group-name>
	</security-role-mapping>

</sun-web-app>
