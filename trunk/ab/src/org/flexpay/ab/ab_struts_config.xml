	<package name="buildings" namespace="/dicts" extends="common">
		<action name="list_buildings" class="listBuildings">
			<interceptor-ref name="commonDefaultStack">
				<param name="flexpayRoles.allowedRoles">BUILDING_READ</param>
			</interceptor-ref>
			<result type="tiles">ab.building.list</result>
		</action>
		<action name="buildingsCreateAction" class="buildingsCreateAction">
			<interceptor-ref name="commonDefaultStack">
				<param name="flexpayRoles.allowedRoles">BUILDING_ADD</param>
			</interceptor-ref>
			<result name="form" type="tiles">ab.buildings.create</result>
			<result name="list" type="redirect-action">list_buildings</result>
			<result name="edit" type="redirect-action">
				<param name="actionName">buildingsEditAction</param>
				<param name="parse">true</param>
				<param name="buildings.id">${buildings.id}</param>
			</result>
		</action>
		<action name="buildingsEditAction" class="buildingsEditAction">
			<interceptor-ref name="commonDefaultStack">
				<param name="flexpayRoles.allowedRoles">BUILDING_CHANGE</param>
			</interceptor-ref>
			<result name="input" type="tiles">ab.buildings.edit</result>
		</action>
		<action name="buildingsDeleteAction" class="buildingsDeleteAction">
			<interceptor-ref name="commonDefaultStack">
				<param name="flexpayRoles.allowedRoles">BUILDING_DELETE</param>
			</interceptor-ref>
			<result name="SUCCESS" type="redirect-action">list_buildings</result>
			<result name="input" type="redirect-action">
				<param name="actionName">buildingsEditAction</param>
				<param name="parse">true</param>
				<param name="buildings.id">${redirectBuildingsId}</param>
			</result>
		</action>
		<action name="buildingsSetPrimaryStatusAction" class="buildingsSetPrimaryStatusAction">
			<interceptor-ref name="commonDefaultStack">
				<param name="flexpayRoles.allowedRoles">BUILDING_CHANGE</param>
			</interceptor-ref>
			<result type="redirect-action">
				<param name="actionName">buildingsEditAction</param>
				<param name="parse">true</param>
				<param name="buildings.id">${redirectBuildingsId!=null ? redirectBuildingsId : buildings.id}</param>
			</result>
		</action>
		
		<action name="buildingAttributeTypeListAction" class="buildingAttributeTypeListAction">
