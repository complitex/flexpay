				<prop key="get*">cacheName=commonFlexpayCache</prop>
				<prop key="findTypeByName">cacheName=commonFlexpayCache</prop>
			</props>
		</property>
		<property name="flushingModels">
			<props>
				<prop key="create">cacheNames=commonFlexpayCache</prop>
