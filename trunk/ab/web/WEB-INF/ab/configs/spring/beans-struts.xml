<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- Define struts actions beans -->

	<!-- Countries -->
	<bean id="countriesListAction" class="org.flexpay.ab.actions.CountriesListAction"
		  scope="prototype">
		<property name="countryService" ref="countryService" />
	</bean>

	<bean id="countryCreateAction" class="org.flexpay.ab.actions.CountryCreateAction"
		  scope="prototype">
		<property name="countryService" ref="countryService" />
	</bean>

	<!-- Regions -->
	<bean id="regionCreateAction"
		  class="org.flexpay.ab.actions.region.RegionCreateAction" scope="prototype">
		<property name="nameTimeDependentService" ref="regionService" />
		<property name="parentService" ref="countryService" />
	</bean>

	<bean id="regionViewAction"
		  class="org.flexpay.ab.actions.region.RegionViewAction" scope="prototype">
		<property name="nameTimeDependentService" ref="regionService" />
	</bean>

	<bean id="regionEditAction" class="org.flexpay.ab.actions.region.RegionEditSimpleAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="regionService" />
	</bean>

	<bean id="regionsListAction" class="org.flexpay.ab.actions.region.RegionsListAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="regionService" />
		<property name="parentService" ref="countryService" />
	</bean>

	<bean id="regionDeleteAction" class="org.flexpay.ab.actions.region.RegionDeleteAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="regionService" />
		<property name="parentService" ref="countryService" />
	</bean>

	<!-- Towns -->
	<bean id="townCreateAction"
		  class="org.flexpay.ab.actions.town.TownCreateAction" scope="prototype">
		<property name="nameTimeDependentService" ref="townService" />
		<property name="parentService" ref="regionService" />
	</bean>

	<bean id="townViewAction"
		  class="org.flexpay.ab.actions.town.TownViewAction" scope="prototype">
		<property name="nameTimeDependentService" ref="townService" />
	</bean>

	<bean id="townEditAction" class="org.flexpay.ab.actions.town.TownEditSimpleAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="townService" />
	</bean>

	<bean id="townsListAction" class="org.flexpay.ab.actions.town.TownsListAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="townService" />
		<property name="parentService" ref="regionService" />
	</bean>

	<bean id="townDeleteAction" class="org.flexpay.ab.actions.town.TownDeleteAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="townService" />
		<property name="parentService" ref="regionService" />
	</bean>

	<!-- Town districts -->
	<bean id="districtCreateAction"  scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictCreateAction"
		  p:nameTimeDependentService-ref="districtService"
		  p:parentService-ref="townService"
		  p:districtService-ref="districtService"
		  p:townService-ref="townService" />

	<bean id="districtViewAction"
		  class="org.flexpay.ab.actions.district.DistrictViewAction" scope="prototype">
		<property name="nameTimeDependentService" ref="districtService" />
	</bean>

	<bean id="districtEditAction"
		  class="org.flexpay.ab.actions.district.DistrictEditSimpleAction" scope="prototype">
		<property name="nameTimeDependentService" ref="districtService" />
	</bean>

	<bean id="districtsListAction" class="org.flexpay.ab.actions.district.DistrictsListAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="districtService" />
		<property name="parentService" ref="townService" />
	</bean>

	<bean id="districtDeleteAction"
		  class="org.flexpay.ab.actions.district.DistrictDeleteAction" scope="prototype">
		<property name="nameTimeDependentService" ref="districtService" />
		<property name="parentService" ref="townService" />
	</bean>

	<!-- Strees -->
	<bean id="streetCreateAction"
		  class="org.flexpay.ab.actions.street.StreetCreateAction" scope="prototype"
		  p:streetTypeService-ref="streetTypeService"
		  p:parentService-ref="townService"
		  p:streetService-ref="streetService"
		  p:townService-ref="townService">
		<property name="nameTimeDependentService" ref="streetService" />
	</bean>

	<bean id="streetViewAction"
		  class="org.flexpay.ab.actions.street.StreetViewAction" scope="prototype">
		<property name="nameTimeDependentService" ref="streetService" />
	</bean>

	<bean id="streetEditAction" class="org.flexpay.ab.actions.street.StreetEditSimpleAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="streetService" />
	</bean>

	<bean id="editStreetTypeAction" class="org.flexpay.ab.actions.street.StreetEditTypeAction"
		  scope="prototype">
		<property name="streetService" ref="streetService" />
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<bean id="streetDistrictEditAction"
		  class="org.flexpay.ab.actions.street.StreetDistrictEditAction" scope="prototype">
		<property name="districtService" ref="districtService" />
		<property name="streetService" ref="streetService" />
	</bean>

	<bean id="streetsListAction" class="org.flexpay.ab.actions.street.StreetsListAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="streetService" />
		<property name="parentService" ref="townService" />
	</bean>

	<bean id="streetDeleteAction" class="org.flexpay.ab.actions.street.StreetDeleteAction"
		  scope="prototype">
		<property name="nameTimeDependentService" ref="streetService" />
		<property name="parentService" ref="townService" />
	</bean>

	<bean id="streetSearchAjaxAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetSearchAjaxAction"
		  p:streetService-ref="streetService" />

	<!-- Buildings -->
	<bean id="buildingsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingsListAction">
		<property name="buildingService" ref="buildingService" />
		<property name="parentService" ref="streetService" />
	</bean>

	<bean id="buildingCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingCreateAction">
		<property name="buildingService" ref="buildingService" />
		<property name="parentService" ref="streetService" />
		<property name="districtService" ref="districtService" />
	</bean>

	<bean id="buildingEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingEditAction"
		  p:buildingService-ref="buildingService"
		  p:addressService-ref="addressService" />

	<bean id="buildingDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingDeleteAction">
		<property name="buildingService" ref="buildingService" />
	</bean>

	<bean id="buildingSetPrimaryStatusAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingSetPrimaryStatusAction">
		<property name="buildingService" ref="buildingService" />
	</bean>

	<!-- BuildingAttributeTypes -->
	<bean id="buildingAttributeTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingAttributeTypesListAction">
		<property name="buildingService" ref="buildingService" />
	</bean>

	<bean id="buildingAttributeTypeCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingAttributeTypeCreateAction">
		<property name="buildingService" ref="buildingService" />
		<property name="languageService" ref="languageService" />
	</bean>

	<bean id="buildingAttributeTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingAttributeTypeEditAction">
		<property name="buildingService" ref="buildingService" />
		<property name="languageService" ref="languageService" />
	</bean>

	<!-- Apartments -->
	<bean id="apartmentsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentsListAction">
		<property name="apartmentService" ref="apartmentService" />
		<property name="parentService" ref="buildingService" />
	</bean>

	<bean id="apartmentEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentEditAction">
		<property name="parentService" ref="buildingService" />
		<property name="apartmentService" ref="apartmentService" />
		<property name="buildingService" ref="buildingService" />
	</bean>

	<bean id="apartmentDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentDeleteAction"
		  p:apartmentService-ref="apartmentService" />

	<bean id="apartmentRegistrationAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentRegistrationAction"
		  p:apartmentService-ref="apartmentService"
		  p:buildingService-ref="buildingService"
		  p:regionService-ref="regionService"
		  p:townService-ref="townService"
		  p:countryService-ref="countryService"
		  p:streetService-ref="streetService" />

	<!-- Town types -->
	<bean id="townTypesListAction"
		  class="org.flexpay.ab.actions.town.TownTypesListAction" scope="prototype">
		<property name="townTypeService" ref="townTypeService" />
	</bean>

	<bean id="townTypeCreateAction"
		  class="org.flexpay.ab.actions.town.TownTypeCreateAction" scope="prototype">
		<property name="townTypeService" ref="townTypeService" />
	</bean>

	<bean id="townTypeViewAction"
		  class="org.flexpay.ab.actions.town.TownTypeViewAction" scope="prototype">
		<property name="townTypeService" ref="townTypeService" />
	</bean>

	<bean id="townTypeEditAction"
		  class="org.flexpay.ab.actions.town.TownTypeEditAction" scope="prototype">
		<property name="townTypeService" ref="townTypeService" />
	</bean>

	<bean id="townTypeDeleteAction"
		  class="org.flexpay.ab.actions.town.TownTypeDeleteAction" scope="prototype">
		<property name="townTypeService" ref="townTypeService" />
	</bean>

	<!-- Street types -->
	<bean id="streetTypesListAction"
		  class="org.flexpay.ab.actions.street.StreetTypesListAction" scope="prototype">
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<bean id="streetTypeCreateAction"
		  class="org.flexpay.ab.actions.street.StreetTypeCreateAction" scope="prototype">
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<bean id="streetTypeViewAction"
		  class="org.flexpay.ab.actions.street.StreetTypeViewAction" scope="prototype">
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<bean id="streetTypeEditAction"
		  class="org.flexpay.ab.actions.street.StreetTypeEditAction" scope="prototype">
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<bean id="streetTypeDeleteAction"
		  class="org.flexpay.ab.actions.street.StreetTypeDeleteAction" scope="prototype">
		<property name="streetTypeService" ref="streetTypeService" />
	</bean>

	<!-- Identity types -->
	<bean id="identityTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypesListAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeCreateAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeViewAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeEditAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeDeleteAction"
		  p:identityTypeService-ref="identityTypeService" />

	<!-- Measure units -->
	<bean id="measureUnitsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitsListAction"
		  p:measureUnitService-ref="measureUnitService" />

	<bean id="measureUnitEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitEditAction"
		  p:measureUnitService-ref="measureUnitService" />

	<bean id="measureUnitDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitDeleteAction"
		  p:measureUnitService-ref="measureUnitService" />

	<!-- Persons -->
	<bean id="personsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonsListAction"
		  p:personService-ref="personService" />

	<bean id="personViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonViewAction"
		  p:personService-ref="personService"
		  p:apartmentService-ref="apartmentService" />

	<bean id="personEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonEditAction"
		  p:parentService-ref="apartmentService"
		  p:personService-ref="personService"
		  p:apartmentService-ref="apartmentService"
		  p:setRegistrationAction-ref="setRegistrationAction" />

	<bean id="personSaveFIOAction" scope="prototype" class="org.flexpay.ab.actions.person.PersonSaveFIOAction"
		  p:personService-ref="personService"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="setRegistrationAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.SetRegistrationAction"
		  p:parentService-ref="apartmentService"
		  p:personService-ref="personService" />

</beans>
