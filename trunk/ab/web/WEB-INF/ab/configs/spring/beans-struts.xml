<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- Define struts actions beans -->

	<!-- Countries -->
	<bean id="countriesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.CountriesListAction"
		  p:countryService-ref="countryService" />

	<bean id="countryCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.CountryCreateAction"
		  p:countryService-ref="countryService" />

	<!-- Regions -->
	<bean id="regionCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.region.RegionCreateAction"
		  p:nameTimeDependentService-ref="regionService"
		  p:parentService-ref="countryService" />

	<bean id="regionViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.region.RegionViewAction"
		  p:nameTimeDependentService-ref="regionService" />

	<bean id="regionEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.region.RegionEditSimpleAction"
		  p:nameTimeDependentService-ref="regionService" />

	<bean id="regionsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.region.RegionsListAction"
		  p:nameTimeDependentService-ref="regionService"
		  p:parentService-ref="countryService" />

	<bean id="regionDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.region.RegionDeleteAction"
		  p:nameTimeDependentService-ref="regionService"
		  p:parentService-ref="countryService" />

	<!-- Towns -->
	<bean id="townViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownViewAction"
		  p:nameTimeDependentService-ref="townService" />

	<bean id="townEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownEditSimpleAction"
		  p:townTypeService-ref="townTypeService"
		  p:townService-ref="townService"
		  p:regionService-ref="regionService" />

	<bean id="townsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownsListAction"
		  p:nameTimeDependentService-ref="townService"
		  p:parentService-ref="regionService" />

	<bean id="townDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownDeleteAction"
		  p:nameTimeDependentService-ref="townService"
		  p:parentService-ref="regionService" />

	<!-- Town districts -->
	<bean id="districtCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictCreateAction"
		  p:nameTimeDependentService-ref="districtService"
		  p:parentService-ref="townService"
		  p:districtService-ref="districtService"
		  p:townService-ref="townService" />

	<bean id="districtViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictViewAction"
		  p:nameTimeDependentService-ref="districtService" />

	<bean id="districtEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictEditSimpleAction"
		  p:nameTimeDependentService-ref="districtService" />

	<bean id="districtsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictsListAction"
		  p:nameTimeDependentService-ref="districtService"
		  p:parentService-ref="townService" />

	<bean id="districtDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.district.DistrictDeleteAction"
		  p:nameTimeDependentService-ref="districtService"
		  p:parentService-ref="townService" />

	<!-- Strees -->
	<bean id="streetCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetCreateAction"
		  p:streetTypeService-ref="streetTypeService"
		  p:parentService-ref="townService"
		  p:streetService-ref="streetService"
		  p:townService-ref="townService"
		  p:nameTimeDependentService-ref="streetService" />

	<bean id="streetViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetViewAction"
		  p:nameTimeDependentService-ref="streetService" />

	<bean id="streetEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetEditSimpleAction"
		  p:nameTimeDependentService-ref="streetService" />

	<bean id="editStreetTypeAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetEditTypeAction"
		  p:streetService-ref="streetService"
		  p:streetTypeService-ref="streetTypeService" />

	<bean id="streetDistrictEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetDistrictEditAction"
		  p:districtService-ref="districtService"
		  p:streetService-ref="streetService" />

	<bean id="streetsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetsListAction"
		  p:streetService-ref="streetService" />

	<bean id="streetDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetDeleteAction"
		  p:nameTimeDependentService-ref="streetService"
		  p:parentService-ref="townService" />

	<bean id="streetSearchAjaxAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetSearchAjaxAction"
		  p:streetService-ref="streetService" />

	<!-- Buildings -->
	<bean id="buildingsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingsListAction"
		  p:buildingService-ref="buildingService"
		  p:parentService-ref="streetService" />

	<bean id="buildingCreateAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingCreateAction"
		  p:buildingService-ref="buildingService"
		  p:buildingAttributeTypeService-ref="addressAttributeTypeService"
		  p:parentService-ref="streetService"
		  p:districtService-ref="districtService" />

	<bean id="buildingEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingEditAction"
		  p:buildingService-ref="buildingService"
		  p:buildingAttributeTypeService-ref="addressAttributeTypeService"
		  p:addressService-ref="addressService" />

	<bean id="buildingDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingDeleteAction"
		  p:buildingService-ref="buildingService" />

	<bean id="buildingSetPrimaryStatusAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.BuildingSetPrimaryStatusAction"
		  p:buildingService-ref="buildingService" />

	<!-- BuildingAttributeTypes -->
	<bean id="addressAttributeTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.AddressAttributeTypesListAction"
		  p:buildingAttributeTypeService-ref="addressAttributeTypeService" />

	<bean id="addressAttributeTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.buildings.AddressAttributeTypeEditAction"
		  p:buildingAttributeTypeService-ref="addressAttributeTypeService" />

	<!-- Apartments -->
	<bean id="apartmentsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentsListAction"
		  p:apartmentService-ref="apartmentService"
		  p:parentService-ref="buildingService" />

	<bean id="apartmentEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentEditAction"
		  p:parentService-ref="buildingService"
		  p:apartmentService-ref="apartmentService"
		  p:buildingService-ref="buildingService" />

	<bean id="apartmentDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentDeleteAction"
		  p:apartmentService-ref="apartmentService" />

	<bean id="apartmentRegistrationAction" scope="prototype"
		  class="org.flexpay.ab.actions.apartment.ApartmentRegistrationAction"
		  p:apartmentService-ref="apartmentService"
		  p:buildingService-ref="buildingService"
		  p:regionService-ref="regionService"
		  p:townService-ref="townService"
		  p:countryService-ref="countryService"
		  p:streetService-ref="streetService" />

	<!-- Town types -->
	<bean id="townTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownTypesListAction"
		  p:townTypeService-ref="townTypeService" />

	<bean id="townTypeViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownTypeViewAction"
		  p:townTypeService-ref="townTypeService" />

	<bean id="townTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownTypeEditAction"
		  p:townTypeService-ref="townTypeService" />

	<bean id="townTypeDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.town.TownTypeDeleteAction"
		  p:townTypeService-ref="townTypeService" />

	<!-- Street types -->
	<bean id="streetTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetTypesListAction"
		  p:streetTypeService-ref="streetTypeService" />

	<bean id="streetTypeViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetTypeViewAction"
		  p:streetTypeService-ref="streetTypeService" />

	<bean id="streetTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetTypeEditAction"
		  p:streetTypeService-ref="streetTypeService" />

	<bean id="streetTypeDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.street.StreetTypeDeleteAction"
		  p:streetTypeService-ref="streetTypeService" />

	<!-- Identity types -->
	<bean id="identityTypesListAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypesListAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeViewAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeEditAction"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="identityTypeDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.identity.IdentityTypeDeleteAction"
		  p:identityTypeService-ref="identityTypeService" />

	<!-- Measure units -->
	<bean id="measureUnitsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitsListAction"
		  p:measureUnitService-ref="measureUnitService" />

	<bean id="measureUnitEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitEditAction"
		  p:measureUnitService-ref="measureUnitService" />

	<bean id="measureUnitDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.measureunit.MeasureUnitDeleteAction"
		  p:measureUnitService-ref="measureUnitService" />

	<!-- Persons -->
	<bean id="personsListAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonsListAction"
		  p:personService-ref="personService" />

	<bean id="personViewAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonViewAction"
		  p:personService-ref="personService"
		  p:apartmentService-ref="apartmentService" />

    <bean id="personDeleteAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonDeleteAction"
		  p:personService-ref="personService"/>

	<bean id="personEditAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonEditAction"
		  p:parentService-ref="apartmentService"
		  p:personService-ref="personService"
		  p:apartmentService-ref="apartmentService"
		  p:setPersonRegistrationAction-ref="setPersonRegistrationAction" />

	<bean id="personSaveFIOAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.PersonSaveFIOAction"
		  p:personService-ref="personService"
		  p:identityTypeService-ref="identityTypeService" />

	<bean id="setPersonRegistrationAction" scope="prototype"
		  class="org.flexpay.ab.actions.person.SetPersonRegistrationAction"
		  p:parentService-ref="apartmentService"
		  p:personService-ref="personService" />

</beans>
