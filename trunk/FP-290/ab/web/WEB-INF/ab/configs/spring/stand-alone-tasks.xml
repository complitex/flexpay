<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean class="org.flexpay.common.util.standalone.StandaloneTasksHolder" factory-method="getInstance">
		<property name="standaloneTasks">
			<list>
				<bean class="org.flexpay.ab.util.standalone.ImportTask">

					<!--Accounts import task for the DataSourceDescription(1L), Town(1L) -->
					<property name="sourceDescription">
						<bean class="org.flexpay.common.persistence.DataSourceDescription">
							<constructor-arg index="0" type="java.lang.Long" value="1" />
						</bean>
					</property>
					<property name="town">
						<bean class="org.flexpay.ab.persistence.Town">
							<constructor-arg index="0" type="java.lang.Long" value="1" />
						</bean>
					</property>
					<property name="importService" ref="importServiceAb" />
				</bean>
			</list>
		</property>
	</bean>

</beans>
