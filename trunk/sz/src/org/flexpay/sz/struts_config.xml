<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
		"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<constant name="struts.custom.i18n.resources"
			  value="org/flexpay/common/i18n/common-messages,org/flexpay/common/i18n/common-errors,
                     org/flexpay/ab/i18n/errors,org/flexpay/ab/i18n/messages
			         org/flexpay/bti/i18n/errors,org/flexpay/bti/i18n/messages,
			         org/flexpay/orgs/i18n/errors,org/flexpay/orgs/i18n/messages,
			  		 org/flexpay/payments/i18n/errors,org/flexpay/payments/i18n/messages,
			  		 org/flexpay/payments/i18n/responseStatuses,
                     org/flexpay/eirc/i18n/errors,org/flexpay/eirc/i18n/messages
                     org/flexpay/sz/i18n/errors,org/flexpay/sz/i18n/messages" />

	<package name="sz" namespace="/sz" extends="common">

		<action name="main" class="menuPageAction">
			<result type="tiles">sz.dictionaries</result>
		</action>

        <action name="mainDicts" class="menuPageAction">
            <result type="tiles">sz.main.dics</result>
        </action>

        <action name="mainImport" class="menuPageAction">
            <result type="tiles">sz.main.import</result>
        </action>

        <action name="mainOtherDicts" class="menuPageAction">
            <result type="tiles">sz.main.other_dics</result>
        </action>

        <action name="mainImportImport" class="menuPageAction">
            <result type="tiles">sz.main.import_import</result>
        </action>

	</package>

    <include file="org/flexpay/sz/actions/szfile/sruts.xml" />

</struts>
