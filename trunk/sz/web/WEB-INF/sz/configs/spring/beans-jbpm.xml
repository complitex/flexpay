<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean name="szFileParserJob" class="org.flexpay.sz.process.szfile.FileParserJob" scope="prototype"
		  p:szFileService-ref="szFileService" 
          p:serviceTypeRecordService-ref="serviceTypeRecordService"
          p:characteristicRecordService-ref="characteristicRecordService"
          p:subsidyRecordService-ref="subsidyRecordService" />

    <bean name="szFileDeleteJob" class="org.flexpay.sz.process.szfile.FileDeleteJob" scope="prototype"
          p:szFileService-ref="szFileService" />

</beans>
