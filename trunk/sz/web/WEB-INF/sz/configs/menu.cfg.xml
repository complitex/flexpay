<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <!--level1-->
    <bean name="sz" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz"
            p:title="sz.menu1.title"
            p:action="/sz/main"
            p:parent-ref="fpMenu"
            p:userRole="ROLE_MENU_SZ"
            />

    <!--level2-->
    <bean name="sz_dicts" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.dicts"
            p:title="sz.menu2.dictionaries"
            p:action="/sz/main"
            p:parent-ref="sz"
            p:userRole="ROLE_MENU_SZ_DICTS"
            />
    <bean name="sz_import" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.import"
            p:title="sz.menu2.import"
            p:action="/sz/import"
            p:parent-ref="sz"
            p:userRole="ROLE_MENU_SZ_IMPORT"
            />

    <!--level3-->
    <bean name="sz_dicts_otherDictionaries" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.other_dicts"
            p:title="sz.menu2.dictionaries"
            p:parent-ref="sz_dicts"
            p:userRole="ROLE_MENU_SZ_OTHER_DICTS"
            />
    <bean name="sz_import_import" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.import2"
            p:title="sz.menu2.import"
            p:parent-ref="sz_import"
            p:userRole="ROLE_MENU_SZ_IMPORT2"
            />

    <!--level4-->
    <bean name="sz_dicts_otherDictionaries_importFilesList" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.import_files_list"
            p:title="sz.menu34.files"
            p:action="/sz/importFilesList"
            p:parent-ref="sz_dicts_otherDictionaries"
            p:userRole="ROLE_MENU_SZ_DICTS_IMPORTED_FILES"
            />
    <bean name="sz_import_import_upload" class="net.sourceforge.navigator.menu.MenuComponent"
            p:name="sz.upload"
            p:title="sz.menu34.files.upload"
            p:action="/sz/doUpload"
            p:parent-ref="sz_import_import"
            p:userRole="ROLE_MENU_SZ_IMPORT_UPLOAD"
            />

</beans>
