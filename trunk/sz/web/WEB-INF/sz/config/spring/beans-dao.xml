<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="importFileDAO" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.sz.dao.ImportFileDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.sz.persistence.ImportFile" />
			</bean>
		</property>
	</bean>
	
	<bean id="osznDAO" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.sz.dao.OsznDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.sz.persistence.Oszn" />
			</bean>
		</property>
	</bean>
	
</beans>	