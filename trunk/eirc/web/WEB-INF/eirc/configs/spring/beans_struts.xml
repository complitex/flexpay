<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="registriesListActionEirc" parent="registriesListAction"
		  p:moduleName="${flexpay.module.name.eirc}"
          scope="prototype" />

	<!-- SP files -->
	<bean id="spFileCreatePageAction" parent="staticPageAction"
		  p:crumbNameKey="eirc.crumbs.sp_file_import"
		  scope="prototype" />

	<bean id="spFileUploadAction"
		  class="org.flexpay.eirc.actions.spfile.SpFileUploadAction"
		  p:fpFileService-ref="fpFileService"
		  p:moduleName="${flexpay.module.name.eirc}"
		  scope="prototype" />

	<bean id="spFilesListPageAction" parent="staticPageAction"
		  p:crumbNameKey="eirc.crumbs.sp_files_list"
		  scope="prototype" />

	<bean id="spFilesListAction"
		  class="org.flexpay.eirc.actions.spfile.SpFilesListAction"
		  p:fpFileService-ref="fpFileService"
		  p:registryFileService-ref="registryFileService"
		  p:moduleName="${flexpay.module.name.eirc}"
		  scope="prototype" />

	<bean id="spFileAction"
		  class="org.flexpay.eirc.actions.spfile.SpFileAction"
		  p:fpFileService-ref="fpFileService"
		  p:processManager-ref="processManager"
		  p:lineParser-ref="mbLineParser"
		  scope="prototype" />

	<!-- Quittances -->
	<bean id="quittanceGenerateAction"
		  class="org.flexpay.eirc.actions.quittance.QuittanceGenerateAction"
		  p:serviceOrganizationService-ref="serviceOrganizationService"
		  p:processManager-ref="processManager"
		  scope="prototype" />

	<bean id="printTicketsAction"
		  class="org.flexpay.eirc.actions.quittance.QuittanceGeneratePDFAction"
		  p:processManager-ref="processManager"
		  p:serviceOrganizationService-ref="serviceOrganizationService"
		  scope="prototype" />

	<bean id="quittanceSearchAction"
		  class="org.flexpay.eirc.actions.quittance.QuittanceSearchAction"
		  p:quittanceService-ref="quittanceService"
		  scope="prototype" />

	<bean id="quittanceSearchByAccountAction"
		  class="org.flexpay.eirc.actions.quittance.QuittanceSearchByAccountAction"
		  p:quittanceService-ref="quittanceService"
		  p:accountService-ref="eircAccountService"
		  scope="prototype" />

	<bean id="quittanceSearchByAddressAction"
		  autowire-candidate="false"
		  class="org.flexpay.eirc.actions.quittance.QuittanceSearchByAddressAction"
		  p:quittanceService-ref="quittanceService"
		  p:eircAccountService-ref="eircAccountService"
		  p:personService-ref="personService"
		  p:parentService-ref="apartmentService"
		  p:addressService-ref="addressService"
		  scope="prototype" />

	<!-- Quittance packets -->
	<bean id="quittancePacketsListAction"
		  class="org.flexpay.eirc.actions.quittance.QuittancePacketsListAction"
		  p:quittancePacketService-ref="quittancePacketService"
		  scope="prototype" />

    <bean id="quittancePacketsListPageAction" parent="staticPageAction"
          p:crumbNameKey="eirc.crumbs.quitance_packets_list"
          scope="prototype" />

	<bean id="quittancePacketEditAction"
		  class="org.flexpay.eirc.actions.quittance.QuittancePacketEditAction"
		  p:paymentPointHelper-ref="paymentPointHelper"
		  p:paymentPointService-ref="paymentPointService"
		  p:quittancePacketService-ref="quittancePacketService"
          p:crumbCreateKey="eirc.crumbs.quitance_packet_create"
          p:crumbNameKey="eirc.crumbs.quitance_packet_edit"
		  scope="prototype" />

	<bean id="quittancePaymentsListAction"
		  class="org.flexpay.eirc.actions.quittance.QuittancePaymentsListAction"
		  p:addressService-ref="addressService"
		  p:eircAccountService-ref="eircAccountService"
		  p:quittancePacketService-ref="quittancePacketService"
          p:crumbNameKey="eirc.crumbs.quitance_packet_view"
		  scope="prototype" />

    <bean id="quittancePacketViewAction"
          class="org.flexpay.eirc.actions.quittance.QuittancePacketViewAction"
          p:crumbNameKey="eirc.crumbs.quitance_packet_view"
          p:quittancePacketService-ref="quittancePacketService"
          scope="prototype" />

	<bean id="quittancePaySearchAction"
		  class="org.flexpay.eirc.actions.quittance.QuittancePaySearchAction"
		  p:quittanceService-ref="quittanceService"
		  scope="prototype" />

	<bean id="quittancePayAction"
		  class="org.flexpay.eirc.actions.quittance.QuittancePayAction"
		  p:quittanceService-ref="quittanceService"
		  p:quittancePaymentService-ref="quittancePaymentService"
		  p:paymentStatusService-ref="quittancePaymentStatusService"
		  p:numberService-ref="quittanceNumberService"
		  scope="prototype" />

	<!-- Served buildings actions -->
	<bean id="serviceOrganizationListServedBuildingsAction"
		  class="org.flexpay.eirc.actions.organization.ServiceOrganizationListServedBuildingsAction"
		  p:serviceOrganizationService-ref="eircServiceOrganizationService"
		  p:addressService-ref="addressService"
          p:streetService-ref="streetService"
		  scope="prototype" />

    <bean id="serviceOrganizationListServedBuildingsPageAction"
          class="org.flexpay.eirc.actions.organization.ServiceOrganizationListServedBuildingsPageAction"
          p:serviceOrganizationService-ref="eircServiceOrganizationService"
          p:crumbNameKey="eirc.crumbs.served_buildings_list"
          scope="prototype" />

    <bean id="serviceOrganizationAddServedBuildingPageAction"
          class="org.flexpay.eirc.actions.organization.ServiceOrganizationAddServedBuildingPageAction"
          p:serviceOrganizationService-ref="eircServiceOrganizationService"
          p:crumbNameKey="eirc.crumbs.add_served_buildings"
          scope="prototype" />

    <bean id="serviceOrganizationBuildingsListAction"
          class="org.flexpay.eirc.actions.organization.ServiceOrganizationBuildingsListAction"
          p:serviceOrganizationService-ref="eircServiceOrganizationService"
          p:streetService-ref="streetService"
          p:addressService-ref="addressService"
          scope="prototype" />

	<bean id="serviceOrganizationAddServedBuildingsAction"
		  class="org.flexpay.eirc.actions.organization.ServiceOrganizationAddServedBuildingsAction"
		  p:serviceOrganizationService-ref="eircServiceOrganizationService"
		  scope="prototype" />

	<bean id="serviceOrganizationRemoveServedBuildingsAction"
		  class="org.flexpay.eirc.actions.organization.ServiceOrganizationRemoveServedBuildingsAction"
		  p:serviceOrganizationService-ref="eircServiceOrganizationService"
		  scope="prototype" />

	<!-- Corrections -->
	<bean id="selectCorrectionTypeAction"
		  class="org.flexpay.eirc.actions.registry.corrections.SelectCorrectionTypeAction"
		  p:registryRecordService-ref="registryRecordService"
		  p:typeRegistry-ref="classToTypeRegistry"
		  scope="prototype" />

	<bean id="setCorrectionAction"
		  class="org.flexpay.eirc.actions.registry.corrections.SetCorrectionAction"
		  p:consumersDataSource-ref="consumersDataSource"
          p:correctionsService-ref="correctionsService"
          p:recordService-ref="registryRecordService"
          p:serviceTypeService-ref="serviceTypeService"
          p:organizationService-ref="organizationService"
          p:paymentPointService-ref="paymentPointService"
          p:cashboxService-ref="cashboxService"
		  scope="prototype" />

    <bean id="registryViewAction"
          class="org.flexpay.eirc.actions.registry.RegistryViewPageAction"
          p:crumbNameKey="payments.crumbs.registry_view"
          p:registryService-ref="registryService"
          p:classToTypeRegistry-ref="classToTypeRegistry"
          p:serviceTypeService-ref="serviceTypeService"
          p:recordStatusService-ref="registryRecordStatusService"
          scope="prototype"  />

    <bean id="registryRecordsListAction"
          class="org.flexpay.payments.actions.registry.RegistryRecordsListAction"
          p:classToTypeRegistry-ref="classToTypeRegistry"
          p:serviceTypeService-ref="serviceTypeService"
          p:registryRecordService-ref="registryRecordService"
          scope="prototype"  />

	<bean id="processesListPageAction"
		  class="org.flexpay.common.actions.processing.ProcessesListPageAction"
		  p:crumbNameKey="eirc.crumbs.processes.list"
		  p:processManager-ref="processManager"
		  scope="prototype" />

	<bean id="processesListAction"
		  class="org.flexpay.common.actions.processing.ProcessesListAction"
		  p:processManager-ref="processManager"
		  scope="prototype" />

	<bean id="processDeleteAction"
		  class="org.flexpay.common.actions.processing.ProcessDeleteAction"
		  p:processManager-ref="processManager"
		  scope="prototype" />

	<bean id="processViewAction"
		  class="org.flexpay.common.actions.processing.ProcessViewAction"
		  p:crumbNameKey="eirc.crumbs.process.view"
		  p:processManager-ref="processManager"
		  scope="prototype" />

	<bean id="processDefinitionDeployAction"
		  class="org.flexpay.common.actions.processing.ProcessDefinitionDeployAction"
		  p:crumbNameKey="eirc.crumbs.process.definition_deploy"
		  p:fpFileService-ref="fpFileService"
		  p:processManager-ref="processManager"
		  p:moduleName="${flexpay.module.name.eirc}"
		  scope="prototype" />

	<bean id="eircAccountsListAction"
		  class="org.flexpay.eirc.actions.eircaccount.EircAccountsListAction"
		  p:eircAccountService-ref="eircAccountService"
		  p:personService-ref="personService"
		  p:addressService-ref="addressService"
		  scope="prototype" />

	<bean id="eircAccountsListPageAction"
		  class="org.flexpay.common.actions.StaticPageAction"
		  p:crumbNameKey="eirc.crumbs.eirc_accounts_list"
		  scope="prototype" />

	<bean id="eircAccountViewAction"
		  class="org.flexpay.eirc.actions.eircaccount.EircAccountViewAction"
		  p:crumbNameKey="eirc.crumbs.eirc_account_view"
		  p:eircAccountService-ref="eircAccountService"
		  p:spService-ref="spService"
		  scope="prototype" />

	<bean id="eircAccountCreateForm1Action"
		  class="org.flexpay.common.actions.StaticPageAction"
		  p:crumbNameKey="eirc.crumbs.eirc_account_creat_form1"
		  scope="prototype" />

	<bean id="eircAccountCreateForm2Action"
		  class="org.flexpay.eirc.actions.eircaccount.EircAccountCreateForm2Action"
		  p:personService-ref="personService"
		  scope="prototype" />

	<bean id="eircAccountCreateAction"
		  class="org.flexpay.eirc.actions.eircaccount.EircAccountCreateAction"
		  p:eircAccountService-ref="eircAccountService"
		  scope="prototype" />

</beans>
