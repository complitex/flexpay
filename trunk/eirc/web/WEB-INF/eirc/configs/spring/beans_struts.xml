<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="spFileCreateAction" class="org.flexpay.eirc.actions.SpFileCreateAction"
		  scope="prototype">
		<property name="spFileService" ref="spFileService" />
	</bean>

	<bean id="spFileListAction" class="org.flexpay.eirc.actions.SpFileListAction"
		  scope="prototype">
		<property name="spFileService" ref="spFileService" />
	</bean>

	<bean id="spFileAction" class="org.flexpay.eirc.actions.SpFileAction"
		  scope="prototype">
		<property name="spFileService" ref="spFileService" />
	</bean>

	<bean id="correspondenceExternalAction" class="org.flexpay.eirc.actions.CorrespondenceExternalAction"
		  scope="prototype">
	</bean>

	<bean id="streetAjaxAction" class="org.flexpay.eirc.actions.StreetAjaxAction"
		  scope="prototype">
		<property name="streetService" ref="streetService" />
	</bean>

	<bean id="buildingAjaxAction" class="org.flexpay.eirc.actions.BuildingAjaxAction"
		  scope="prototype">
		<property name="buildingService" ref="buildingService" />
	</bean>


	<bean id="generateTicketsAction" class="org.flexpay.eirc.actions.GenerateTicketAction"
		  scope="prototype">
		<property name="serviceOrganisationService" ref="serviceOrganisationService" />
		<property name="tickerService" ref="ticketService" />
	</bean>

	<bean id="printTicketsAction" class="org.flexpay.eirc.actions.PrintTicketAction"
		  scope="prototype">
		<property name="serviceOrganisationService" ref="serviceOrganisationService" />
		<property name="ticketService" ref="ticketService" />
	</bean>

	<bean id="ticketPayFormAction" class="org.flexpay.eirc.actions.TicketPayFormAction"
		  scope="prototype">
		<property name="ticketService" ref="ticketService" />
	</bean>

	<bean id="ticketPayAction" class="org.flexpay.eirc.actions.TicketPayAction"
		  scope="prototype">
		<property name="ticketService" ref="ticketService" />
	</bean>

	<bean id="listRegistriesAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.ListRegistriesAction">
		<property name="registryService" ref="spRegistryService" />
		<property name="organisationService" ref="organisationService" />
		<property name="registryTypeService" ref="spRegistryTypeService" />
	</bean>

	<bean id="processRegistriesAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.ProcessRegistriesAction">
		<property name="providerFileProcessor" ref="serviceProviderFileProcessor" />
		<property name="registryService" ref="spRegistryService" />
	</bean>

	<bean id="processRegistryRecordsAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.ProcessRegistryRecordsAction">
		<property name="providerFileProcessor" ref="serviceProviderFileProcessor" />
		<property name="registryRecordService" ref="spRegistryRecordService" />
		<property name="registryService" ref="spRegistryService" />
	</bean>

	<bean id="showRegistryAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.ShowRegistryAction">
		<property name="registryService" ref="spRegistryService" />
		<property name="registryRecordService" ref="spRegistryRecordService" />
		<property name="classToTypeRegistry" ref="typeRegistryEirc" />
		<property name="serviceTypeService" ref="serviceTypeService" />
	</bean>

	<bean id="listOrganisationsAction" scope="prototype"
		  class="org.flexpay.eirc.actions.organisation.ListOrganisationsAction"
		  p:organisationService-ref="organisationService" />

	<bean id="deleteOrganisationsAction" scope="prototype"
		  class="org.flexpay.eirc.actions.organisation.DeleteOrganisationsAction"
		  p:organisationService-ref="organisationService" />

	<bean id="editOrganisationAction" scope="prototype"
		  class="org.flexpay.eirc.actions.organisation.EditOrganisationAction"
		  p:organisationService-ref="organisationService" />

	<bean id="listServiceProvidersAction" scope="prototype"
		  class="org.flexpay.eirc.actions.serviceprovider.ListServiceProvidersAction"
		  p:spService-ref="spService" />

	<bean id="deleteServiceProvidersAction" scope="prototype"
		  class="org.flexpay.eirc.actions.serviceprovider.DeleteServiceProvidersAction"
		  p:spService-ref="spService" />

	<bean id="editServiceProviderAction" scope="prototype"
		  class="org.flexpay.eirc.actions.serviceprovider.EditServiceProviderAction"
		  p:organisationService-ref="organisationService"
		  p:spService-ref="spService" />


	<!-- Services -->
	<bean id="listServiceTypesAction" scope="prototype"
		  class="org.flexpay.eirc.actions.service.ListServiceTypesAction"
		  p:serviceTypeService-ref="serviceTypeService" />

	<bean id="deleteServiceTypesAction" scope="prototype"
		  class="org.flexpay.eirc.actions.service.DeleteServiceTypesAction"
		  p:serviceTypeService-ref="serviceTypeService" />

	<bean id="editServiceTypeAction" scope="prototype"
		  class="org.flexpay.eirc.actions.service.EditServiceTypeAction"
		  p:serviceTypeService-ref="serviceTypeService" />

	<bean id="listServicesAction" scope="prototype"
		  class="org.flexpay.eirc.actions.service.ListServicesAction"
		  p:spService-ref="spService" />

	<bean id="editServiceAction" scope="prototype"
		  class="org.flexpay.eirc.actions.service.EditServiceAction"
		  p:serviceTypeService-ref="serviceTypeService"
		  p:spService-ref="spService" />


	<!-- Corrections -->
	<bean id="selectCorrectionTypeAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.corrections.SelectTypeAction">
		<property name="registryRecordService" ref="spRegistryRecordService" />
		<property name="typeRegistry" ref="typeRegistryEirc" />
	</bean>

	<bean id="correctRecordAddressAction" scope="prototype" parent="listApartments"
		  class="org.flexpay.eirc.actions.registry.corrections.CorrectAddressAction">
		<property name="consumersDataSource" ref="consumersDataSource" />
		<property name="correctionsService" ref="correctionsServiceEirc" />
		<property name="recordService" ref="spRegistryRecordService" />
		<property name="serviceTypeService" ref="serviceTypeService" />
		<property name="typeRegistry" ref="typeRegistryEirc" />
	</bean>

	<bean id="correctBuildingAddressAction" scope="prototype" parent="listBuildings"
		  class="org.flexpay.eirc.actions.registry.corrections.CorrectBuildingAction">
		<property name="consumersDataSource" ref="consumersDataSource" />
		<property name="correctionsService" ref="correctionsServiceEirc" />
		<property name="recordService" ref="spRegistryRecordService" />
		<property name="serviceTypeService" ref="serviceTypeService" />
	</bean>

	<bean id="correctStreetAddressAction" scope="prototype" parent="listStreetsAction"
		  class="org.flexpay.eirc.actions.registry.corrections.CorrectStreetAction">
		<property name="consumersDataSource" ref="consumersDataSource" />
		<property name="correctionsService" ref="correctionsServiceEirc" />
		<property name="recordService" ref="spRegistryRecordService" />
		<property name="serviceTypeService" ref="serviceTypeService" />
	</bean>

	<bean id="correctRecordPersonAction" scope="prototype"
		  class="org.flexpay.eirc.actions.registry.corrections.CorrectPersonAction">
	</bean>

</beans>