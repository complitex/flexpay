<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="serviceDaoExt" class="org.flexpay.eirc.dao.imp.ServiceDaoExtImpl"
		  p:hibernateTemplate-ref="hibernateTemplate" />

	<bean id="serviceDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ServiceDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Service" />
			</bean>
		</property>
	</bean>

	<bean id="serviceTypeDao" parent="abstractDao" p:proxyInterfaces="org.flexpay.eirc.dao.ServiceTypeDao">
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.ServiceType" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryDaoExt" class="org.flexpay.eirc.dao.imp.RegistryDaoExtImpl"
		  p:hibernateTemplate-ref="hibernateTemplate" />

	<bean id="registryFileDaoExt" class="org.flexpay.common.dao.registry.impl.RegistryFileDaoExtImpl"
		  p:hibernateTemplate-ref="hibernateTemplate" />


	<bean id="spRegistryDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.Registry" />
			</bean>
		</property>
	</bean>

	<bean id="registryContainerDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryContainerDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryContainer" />
			</bean>
		</property>
	</bean>

	<bean id="registryRecordContainerDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryRecordContainerDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryRecordContainer" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryRecordDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryRecord" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordDaoExt"
		  class="org.flexpay.common.dao.registry.impl.RegistryRecordDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="spRegistryTypeDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryTypeDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryType" />
			</bean>
		</property>
	</bean>

	<bean id="consumerDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ConsumerDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Consumer" />
			</bean>
		</property>
	</bean>

	<bean id="consumerDaoExt" class="org.flexpay.eirc.dao.imp.ConsumersDaoExtJdbcImpl"
		  p:dataSource-ref="dataSource"
		  p:hibernateTemplate-ref="hibernateTemplate" />

	<bean id="eircAccountDaoExt" class="org.flexpay.eirc.dao.imp.EircAccountDaoExtImpl"
		  p:hibernateTemplate-ref="hibernateTemplate" />

	<bean id="quittanceDaoExt" class="org.flexpay.eirc.dao.imp.QuittanceDaoExtImpl"
		  p:jdbcTemplate-ref="jdbcTemplate" />

	<bean id="eircAccountDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.EircAccountDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.EircAccount" />
			</bean>
		</property>
	</bean>

	<bean id="consumerInfoDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ConsumerInfoDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.ConsumerInfo" />
			</bean>
		</property>
	</bean>

	<bean id="servedBuildingDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ServedBuildingDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.ServedBuilding" />
			</bean>
		</property>
	</bean>

	<bean id="quittanceDetailsDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.QuittanceDetailsDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.account.QuittanceDetails" />
			</bean>
		</property>
	</bean>

	<bean id="quittanceDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.QuittanceDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.account.Quittance" />
			</bean>
		</property>
	</bean>

	<bean id="quittancePacketDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.QuittancePacketDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.QuittancePacket" />
			</bean>
		</property>
	</bean>

	<bean id="quittancePacketDaoExt" class="org.flexpay.eirc.dao.imp.QuittancePacketDaoExtImpl"
		  p:hibernateTemplate-ref="hibernateTemplate" />

	<bean id="quittancePaymentDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.QuittancePaymentDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.QuittancePayment" />
			</bean>
		</property>
	</bean>

	<bean id="quittancePaymentStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.QuittancePaymentStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.QuittancePaymentStatus" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryStatus" />
			</bean>
		</property>
	</bean>

	<bean id="registryArchiveStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryArchiveStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryArchiveStatus" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.common.dao.registry.RegistryRecordStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.common.persistence.registry.RegistryRecordStatus" />
			</bean>
		</property>
	</bean>

</beans>
