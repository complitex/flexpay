<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="accountStatusDao" class="org.flexpay.eirc.dao.imp.AccountStatusDaoImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="serviceDaoExt" class="org.flexpay.eirc.dao.imp.ServiceDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="personalAccountDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.PersonalAccountDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.PersonalAccount" />
			</bean>
		</property>
	</bean>
	
	<bean id="spFileDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpFileDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpFile" />
			</bean>
		</property>
	</bean>
	
	<bean id="spRegistryDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistry" />
			</bean>
		</property>
	</bean>
	
	<bean id="spRegistryRecordDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryRecordDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryRecord" />
			</bean>
		</property>
	</bean>
	
	<bean id="spRegistryTypeDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryTypeDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryType" />
			</bean>
		</property>
	</bean>

</beans>
