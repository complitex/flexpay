<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<import resource="beans_dao.xml" />

	<bean id="accountStatusService"
		  class="org.flexpay.eirc.service.imp.AccountStatusServiceImpl">
		<property name="accountStatusDao" ref="accountStatusDao" />
	</bean>

	<bean id="personalAccountService"
		  class="org.flexpay.eirc.service.imp.PersonalAccountServiceImpl">
		<property name="personalAccountDao" ref="personalAccountDao" />
	</bean>
	
	<bean id="spFileService"
		  class="org.flexpay.eirc.service.imp.SpFileServiceImpl">
		<property name="spFileDao" ref="spFileDao" />
	</bean>

</beans>
