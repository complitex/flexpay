<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5">
	<!--eirc-->
	<context-param>
		<param-name>webAppRootKey</param-name>
		<param-value>FlexPay EIRC module COMMON_INSTANCE</param-value>
	</context-param>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
			/WEB-INF/applicationContext.xml
			/WEB-INF/applicationContext-@@@SECURITYPLACEHOLDER@@@.xml
		</param-value>
	</context-param>

	<!--
		- Location of the Log4J config file, for initialization and refresh checks.
		- Applied by Log4jConfigListener.
	-->
	<context-param>
		<param-name>log4jConfigLocation</param-name>
		<param-value>/WEB-INF/common/configs/log4j.xml</param-value>
	</context-param>
	<context-param>
		<param-name>definitions-config</param-name>
		<param-value>
			/WEB-INF/common/configs/tiles_common.xml,
			/WEB-INF/ab/configs/tiles_ab.xml,
			/WEB-INF/admin/configs/tiles_admin.xml,
			/WEB-INF/bti/configs/tiles_bti.xml,
			/WEB-INF/orgs/configs/tiles_orgs.xml,
			/WEB-INF/payments/configs/tiles_payments.xml,
			/WEB-INF/admin/configs/tiles_payments_admin.xml,
			/WEB-INF/eirc/configs/tiles_eirc.xml
		</param-value>
	</context-param>

	<listener>
		<listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
	</listener>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<listener>
		<listener-class>org.springframework.security.ui.session.HttpSessionEventPublisher</listener-class>
	</listener>
	<listener>
		<listener-class>org.apache.struts2.tiles.StrutsTilesListener</listener-class>
	</listener>
    <listener>
        <listener-class>net.sourceforge.navigator.menu.MenuContextListener</listener-class>
    </listener>

	<filter>
		<filter-name>CharacterEncodingFilter</filter-name>
		<filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
		<init-param>
			<param-name>forceEncoding</param-name>
			<param-value>true</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>CharacterEncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<filter>
		<filter-name>springSecurityFilterChain</filter-name>
		<filter-class>org.springframework.web.filter.DelegatingFilterProxy</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>springSecurityFilterChain</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<filter>
		<filter-name>actionFilter</filter-name>
		<filter-class>org.apache.struts2.dispatcher.FilterDispatcher</filter-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>
				struts-default.xml,struts-plugin.xml,
				org/flexpay/common/struts_config.xml,
				org/flexpay/ab/struts_config.xml,
				org/flexpay/admin/struts_config.xml,
				org/flexpay/bti/struts_config.xml,
				org/flexpay/orgs/struts_config.xml,
				org/flexpay/payments/struts_config.xml,
				org/flexpay/eirc/struts_config.xml
			</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>actionFilter</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>FORWARD</dispatcher>
		<dispatcher>INCLUDE</dispatcher>
		<dispatcher>ERROR</dispatcher>
	</filter-mapping>

	<servlet>
		<servlet-name>spring-ws</servlet-name>
		<servlet-class>org.springframework.ws.transport.http.MessageDispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>spring-ws</servlet-name>
		<url-pattern>/ws/*</url-pattern>
	</servlet-mapping>

	<!-- Log file download servlet -->
	<servlet>
		<servlet-name>processLogDownloadServlet</servlet-name>
		<servlet-class>org.flexpay.common.actions.ProcessLogDownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>processLogDownloadServlet</servlet-name>
		<url-pattern>/processLogDownloadServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>fileDownloadServlet</servlet-name>
		<servlet-class>org.flexpay.common.actions.FileDownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileDownloadServlet</servlet-name>
		<url-pattern>/download/*</url-pattern>
	</servlet-mapping>

    <servlet>
        <servlet-name>confirmationTradingDayServlet</servlet-name>
        <servlet-class>org.flexpay.payments.actions.tradingday.ConfirmationTradingDayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
		<servlet-name>confirmationTradingDayServlet</servlet-name>
		<url-pattern>/confirmationTradingDay/*</url-pattern>
	</servlet-mapping>

    <servlet>
        <servlet-name>processRequestServlet</servlet-name>
        <servlet-class>org.flexpay.payments.actions.outerrequest.OuterRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>processRequestServlet</servlet-name>
        <url-pattern>/process/*</url-pattern>
    </servlet-mapping>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>secure-resource</web-resource-name>
			<url-pattern>*.action</url-pattern>
			<http-method>POST</http-method>
			<http-method>GET</http-method>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>secure-resource</web-resource-name>
			<url-pattern>/download/*</url-pattern>
			<http-method>POST</http-method>
			<http-method>GET</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
	</security-constraint>

