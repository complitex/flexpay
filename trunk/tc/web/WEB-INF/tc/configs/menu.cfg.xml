<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <!--level1-->
    <bean name="tc" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc"
            p:title="tc.menu1.title"
            p:action="/tc/main.action?menu=tc"
            p:parent-ref="fpMenu"
            p:userRole="ROLE_BASIC"
            />

    <!--level2-->
    <bean name="tc_dicts" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.dicts"
            p:title="tc.menu2.dictionaries"
            p:action="/tc/main.action?menu=tc.dicts"
            p:parent-ref="tc"
            p:userRole="ROLE_BASIC"
            />
    <bean name="tc_importexport" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.importexport"
            p:title="tc.menu2.importexport"
            p:action="/tc/importexport.action?menu=tc.importexport"
            p:parent-ref="tc"
            p:userRole="ROLE_BASIC"
            />
    <bean name="tc_data" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.data"
            p:title="tc.menu2.data"
            p:action="/tc/data.action?menu=tc.data"
            p:parent-ref="tc"
            p:userRole="ROLE_BASIC"
            />

    <!--level3-->
    <bean name="tc_dicts_otherDictionaries" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.other_dicts"
            p:title="tc.menu2.dictionaries"
            p:parent-ref="tc_dicts"
            p:userRole="ROLE_BASIC"
            />
    <bean name="tc_importexport_importexport" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.importexport2"
            p:title="tc.menu2.importexport"
            p:parent-ref="tc_importexport"
            p:userRole="ROLE_BASIC"
            />
    <bean name="tc_importexport_processes" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.processes"
            p:title="tc.menu34.processes"
            p:parent-ref="tc_importexport"
            p:userRole="ROLE_BASIC"
            />
    <bean name="tc_data_data" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.data2"
            p:title="tc.menu2.data"
            p:parent-ref="tc_data"
            p:userRole="ROLE_BASIC"
            />

    <!--level4-->
    <bean name="tc_dicts_otherDictionaries_rulesFiles" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.rules_files"
            p:title="tc.menu34.rules_files"
            p:action="/tc/rulesFilesList.action?menu=tc.rules_files"
            p:parent-ref="tc_dicts_otherDictionaries"
            p:userRole="ROLE_TC_TARIFF_CALCULATION_RULES_FILE_READ"
            />
    <bean name="tc_dicts_otherDictionaries_buildingAttributeTypes" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.building_attribute_types"
            p:title="bti.building.attribute.types"
            p:action="/tc/buildingAttributeTypesList.action?menu=tc.building_attribute_types"
            p:parent-ref="tc_dicts_otherDictionaries"
            p:userRole="ROLE_AB_BUILDING_ATTRIBUTE_TYPE_READ"
            />
    <bean name="tc_importexport_importexport_calcResultExport" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.calc_result_export"
            p:title="tc.menu34.calc_results_export"
            p:action="/tc/calcResultExport.action?menu=tc.calc_result_export"
            p:parent-ref="tc_importexport_importexport"
            p:userRole="ROLE_TC_TARIFF_CALCULATION_RESULT_READ"
            />
    <bean name="tc_importexport_importexport_attributesImport" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.attributes_import"
            p:title="bti.building.attribute.import"
            p:action="/tc/buildingAttributesImport.action?menu=tc.attributes_import"
            p:parent-ref="tc_importexport_importexport"
            p:userRole=""
            />
    <bean name="tc_importexport_processes_processesList" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.processes_list"
            p:title="common.processing.process_list"
            p:action="/tc/processesList.action?menu=tc.processes_list"
            p:parent-ref="tc_importexport_processes"
            p:userRole=""
            />
    <bean name="tc_importexport_processes_processDefinitionDeploy" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.process_definition"
            p:title="common.processing.definition"
            p:action="/tc/processDefinitionDeploy.action?menu=tc.process_definition"
            p:parent-ref="tc_importexport_processes"
            p:userRole=""
            />
    <bean name="tc_data_data_buildingAttributes" class="net.sf.navigator.menu.MenuComponent"
            p:name="tc.building_attributes"
            p:title="tc.menu34.building_attributes"
            p:action="/tc/buildingsList.action?menu=tc.building_attributes"
            p:parent-ref="tc_data_data"
            p:userRole=""
            />

</beans>
