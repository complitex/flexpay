<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="tc-sewermaterialtypes" namespace="/tc" extends="common">
        <action name="sewerMaterialTypesList" class="sewerMaterialTypesListAction">
            <result type="tiles">tc.sewer_material_types.list</result>
        </action>

        <action name="sewerMaterialTypeDelete" class="sewerMaterialTypeDeleteAction">
            <result name="redirectSuccess" type="redirect-action">sewerMaterialTypesList</result>
        </action>

        <action name="sewerMaterialTypeEdit" class="sewerMaterialTypeEditAction">
            <result name="input" type="tiles">tc.sewer_material_type.edit</result>
            <result name="redirectSuccess" type="redirect-action">sewerMaterialTypesList</result>
        </action>

    </package>

</struts>