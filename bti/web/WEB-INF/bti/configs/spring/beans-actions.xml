<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="buildingAttributeTypesListAction" scope="prototype"
		  class="org.flexpay.bti.actions.buildings.BuildingAttributeTypesListAction"
		  p:attributeTypeService-ref="buildingAttributeTypeService" />

	<bean id="buildingAttributeTypeEditAction" scope="prototype"
		  class="org.flexpay.bti.actions.buildings.BuildingAttributeTypeEditAction"
		  p:attributeGroupService-ref="buildingAttributeGroupService"
		  p:attributeTypeService-ref="buildingAttributeTypeService" />

	<bean id="buildingAttributesImportAction" scope="prototype"
		  class="org.flexpay.bti.actions.buildings.BuildingAttributesImportAction"
		  p:fpFileService-ref="fpFileService"
		  p:processManager-ref="processManager" />

</beans>
