		  p:userRole="ROLE_MENU_AB_DICTS_PERSONS"
			/>
	<bean name="ab_dicts_typeDictionaries_townTypes" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.town_types"
		  p:title="ab.menu.dict.town_types"
		  p:action="/dicts/townTypesList.action?menu=ab.town_types"
		  p:parent-ref="ab_dicts_typeDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_TOWN_TYPES"
			/>
	<bean name="ab_dicts_typeDictionaries_streetTypes" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.street_types"
		  p:title="ab.menu.dict.street_types"
		  p:action="/dicts/streetTypesList.action?menu=ab.street_types"
		  p:parent-ref="ab_dicts_typeDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_STREET_TYPES"
			/>
	<bean name="ab_dicts_typeDictionaries_identityTypes" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.identity_types"
		  p:title="ab.menu.dict.identity_types"
		  p:action="/dicts/identityTypesList.action?menu=ab.identity_types"
		  p:parent-ref="ab_dicts_typeDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_IDENTITY_TYPES"
			/>
	<bean name="ab_dicts_typeDictionaries_attributeTypes" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.attribute_types"
		  p:title="ab.menu.dict.attribute_types"
		  p:action="/dicts/addressAttributeTypesList.action?menu=ab.attribute_types"
		  p:parent-ref="ab_dicts_typeDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_BUILDING_ATTRIBUTE_TYPES"
			/>
	<bean name="ab_dicts_typeDictionaries_measureUnits" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.measure_units"
		  p:title="ab.menu.dict.measure_units"
		  p:action="/dicts/measureUnitsList.action?menu=ab.measure_units"
