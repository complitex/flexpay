<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!--level1-->
	<bean name="ab" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab"
		  p:title="ab.menu.ab"
		  p:action="/ab/main.action?menu=ab"
		  p:parent-ref="fpMenu"
		  p:userRole="ROLE_MENU_AB"
			/>

	<!--level2-->
	<bean name="ab_dicts" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.dicts"
		  p:title="ab.menu.dicts"
		  p:action="/ab/main.action?menu=ab.dicts"
		  p:parent-ref="ab"
		  p:userRole="ROLE_MENU_AB_DICTS"
			/>

	<!--level3-->
	<bean name="ab_dicts_addressDictionaries" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.address_dictionaries"
		  p:title="ab.menu.address_dictionaries"
		  p:parent-ref="ab_dicts"
		  p:userRole="ROLE_MENU_AB_ADDRESS_DICTS"
			/>
	<bean name="ab_dicts_personDictionaries" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.person_dictionaries"
		  p:title="ab.menu.person_dictionaries"
		  p:parent-ref="ab_dicts"
		  p:userRole="ROLE_MENU_AB_PERSON_DICTS"
			/>
	<bean name="ab_dicts_typeDictionaries" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.type_dictionaries"
		  p:title="ab.menu.type_dictionaries"
		  p:parent-ref="ab_dicts"
		  p:userRole="ROLE_MENU_AB_TYPE_DICTS"
			/>
	<bean name="ab_dicts_otherDictionaries" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.other_dictionaries"
		  p:title="ab.menu.other_dictionaries"
		  p:parent-ref="ab_dicts"
		  p:userRole="ROLE_MENU_AB_OTHER_DICTS"
			/>

	<!--level4-->
	<bean name="ab_dicts_addressDictionaries_counties" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.counties"
		  p:title="ab.menu.dict.countries"
		  p:action="/dicts/countriesList.action?menu=ab.counties"
		  p:parent-ref="ab_dicts_addressDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_COUNTRIES"
			/>
	<bean name="ab_dicts_addressDictionaries_regions" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.regions"
		  p:title="ab.menu.dict.regions"
		  p:action="/dicts/regionsList.action?menu=ab.regions"
		  p:parent-ref="ab_dicts_addressDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_REGIONS"
			/>
	<bean name="ab_dicts_addressDictionaries_towns" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.towns"
		  p:title="ab.menu.dict.towns"
		  p:action="/dicts/townsList.action?menu=ab.towns"
		  p:parent-ref="ab_dicts_addressDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_TOWNS"
			/>
	<bean name="ab_dicts_addressDictionaries_districts" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.districts"
		  p:title="ab.menu.dict.districts"
		  p:action="/dicts/districtsList.action?menu=ab.districts"
		  p:parent-ref="ab_dicts_addressDictionaries"
		  p:userRole="ROLE_MENU_AB_DICTS_DISTRICTS"
			/>
	<bean name="ab_dicts_addressDictionaries_streets" class="net.sf.navigator.menu.MenuComponent"
		  p:name="ab.streets"
