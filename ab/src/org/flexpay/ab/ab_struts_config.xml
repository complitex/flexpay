<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
		"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<constant name="struts.custom.i18n.resources"
			  value="org/flexpay/common/i18n/common-messages,org/flexpay/ab/i18n/errors,org/flexpay/ab/i18n/messages" />

	<package name="ab" namespace="/ab" extends="common">

		<action name="main">
			<interceptor-ref name="flexpayRoles">
				<param name="allowedRoles">developerRole</param>
			</interceptor-ref>
			<result type="tiles">ab.main</result>
		</action>

	</package>

	<package name="lang" namespace="/" extends="tiles-default">

		<interceptors>
			<interceptor name="i18n_flexpay"
						 class="org.flexpay.common.actions.interceptor.FlexpayI18nInterceptor" />
		</interceptors>

		<action name="set_language" class="org.flexpay.common.actions.LanguageSwitch">
			<result type="tiles">ab.main</result>
			<interceptor-ref name="i18n_flexpay" />
		</action>

	</package>

	<package name="countries" namespace="/dicts" extends="common">

		<action name="list_countries" class="listCountriesAction">
			<result type="tiles">ab.country.list</result>
		</action>

		<action name="create_country" class="createCountryAction">
			<result type="redirect">list_countries.action</result>
			<result type="tiles" name="input">ab.country.create</result>
		</action>

	</package>

	<package name="regions" namespace="/dicts" extends="common">

		<action name="list_regions" class="listRegionsAction">
			<result type="tiles">ab.regions.list</result>
		</action>

		<action name="create_region" class="createRegionAction">
			<result type="redirect-action">
				<param name="actionName">view_region</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.region.create</result>
		</action>

		<action name="view_region" class="viewRegionAction">
			<result type="tiles">ab.region.view</result>
			<result type="redirect" name="error">list_regions.action</result>
		</action>

		<action name="edit_region" class="editRegionAction">
			<result type="redirect-action">
				<param name="actionName">view_region</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.region.edit</result>
		</action>

		<action name="delete_regions" class="deleteRegionsAction">
			<result type="redirect">list_regions.action</result>
		</action>

	</package>

	<package name="towns" namespace="/dicts" extends="common">
		<action name="list_towns" class="listTownsAction">
			<result type="tiles">ab.towns.list</result>
		</action>

		<action name="create_town" class="createTownAction">
			<result type="redirect-action">
				<param name="actionName">view_town</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.town.create</result>
		</action>

		<action name="view_town" class="viewTownAction">
			<result type="tiles">ab.town.view</result>
			<result type="redirect" name="error">list_towns.action</result>
		</action>

		<action name="edit_town" class="editTownAction">
			<result type="redirect-action">
				<param name="actionName">view_town</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.town.edit</result>
		</action>

		<action name="delete_towns" class="deleteTownsAction">
			<result type="redirect">list_towns.action</result>
		</action>
	</package>

	<package name="town_types" namespace="/dicts" extends="common">
		<action name="town_type_create" class="townTypeCreteAction">
			<result name="form" type="tiles">ab.town_type.create</result>
			<result name="afterSubmit" type="redirect">town_type_list.action</result>
		</action>
		<action name="town_type_view" class="townTypeViewAction">
			<result name="success" type="tiles">ab.town_type.view</result>
		</action>
		<action name="town_type_list" class="townTypeListAction">
			<result name="list" type="tiles">ab.town_type.list</result>
		</action>
		<action name="town_type_edit" class="townTypeEditAction">
			<result name="form" type="tiles">ab.town_type.edit</result>
			<result name="afterSubmit" type="redirect">town_type_list.action</result>
		</action>
		<action name="town_type_delete" class="townTypeDeleteAction">
			<result name="afterSubmit" type="redirect">town_type_list.action</result>
		</action>
	</package>

	<package name="districts" namespace="/dicts" extends="common">
		<action name="list_districts" class="listDistrictsAction">
			<result type="tiles">ab.districts.list</result>
		</action>

		<action name="create_district" class="createDistrictAction">
			<result type="redirect-action">
				<param name="actionName">view_district</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.district.create</result>
		</action>

		<action name="view_district" class="viewDistrictAction">
			<result type="tiles">ab.district.view</result>
			<result type="redirect" name="error">list_districts.action</result>
		</action>

		<action name="edit_district" class="editDistrictAction">
			<result type="redirect-action">
				<param name="actionName">view_district</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.district.edit</result>
		</action>

		<action name="delete_districts" class="deleteDistrictsAction">
			<result type="redirect">list_districts.action</result>
		</action>
	</package>

	<package name="streets" namespace="/dicts" extends="common">
		<action name="list_streets" class="listStreetsAction">
			<result type="tiles">ab.streets.list</result>
		</action>

		<action name="create_street" class="createStreetAction">
			<result type="redirect-action">
				<param name="actionName">view_street</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.street.create</result>
		</action>

		<action name="view_street" class="viewStreetAction">
			<result type="tiles">ab.street.view</result>
			<result type="redirect" name="error">list_streets.action</result>
		</action>

		<action name="edit_street" class="editStreetAction">
			<result type="redirect-action">
				<param name="actionName">view_street</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.street.edit</result>
		</action>

		<action name="edit_street_type" class="editStreetTypeAction">
			<result type="redirect-action">
				<param name="actionName">view_street</param>
				<param name="parse">true</param>
				<param name="object.id">${object.id}</param>
			</result>
			<result type="tiles" name="input">ab.street.edit.type</result>
		</action>

		<action name="delete_streets" class="deleteStreetsAction">
			<result type="redirect">list_streets.action</result>
		</action>

		<action name="edit_street_districts" class="editStreetDistricts">
			<result type="tiles" name="input">ab.street.edit.districts</result>
			<result type="redirect-action">
				<param name="actionName">view_street</param>
				<param name="parse">true</param>
				<param name="object.id">${street.id}</param>
			</result>
		</action>
	</package>

	<package name="street_types" namespace="/dicts" extends="common">
		<action name="street_type_create" class="streetTypeCreteAction">
			<result name="form" type="tiles">ab.street_type.create</result>
			<result name="afterSubmit" type="redirect">street_type_list.action</result>
		</action>
		<action name="street_type_view" class="streetTypeViewAction">
			<result name="success" type="tiles">ab.street_type.view</result>
		</action>
		<action name="street_type_list" class="streetTypeListAction">
			<result name="list" type="tiles">ab.street_type.list</result>
		</action>
		<action name="street_type_edit" class="streetTypeEditAction">
			<result name="form" type="tiles">ab.street_type.edit</result>
			<result name="afterSubmit" type="redirect">street_type_list.action</result>
		</action>
		<action name="street_type_delete" class="streetTypeDeleteAction">
			<result name="afterSubmit" type="redirect">street_type_list.action</result>
		</action>
	</package>

	<package name="identity_types" namespace="/dicts" extends="common">
		<action name="identity_type_create" class="identityTypeCreteAction">
			<result name="form" type="tiles">ab.identity_type.create</result>
			<result name="afterSubmit" type="redirect">identity_type_list.action</result>
		</action>
		<action name="identity_type_view" class="identityTypeViewAction">
			<result name="success" type="tiles">ab.identity_type.view</result>
		</action>
		<action name="identity_type_list" class="identityTypeListAction">
			<result name="list" type="tiles">ab.identity_type.list</result>
		</action>
		<action name="identity_type_edit" class="identityTypeEditAction">
			<result name="form" type="tiles">ab.identity_type.edit</result>
			<result name="afterSubmit" type="redirect">identity_type_list.action</result>
		</action>
		<action name="identity_type_delete" class="identityTypeDeleteAction">
			<result name="afterSubmit" type="redirect">identity_type_list.action</result>
		</action>
	</package>

	<package name="persons" namespace="/dicts" extends="common">
		<action name="list_persons" class="listPersons">
			<result type="tiles">ab.person.list</result>
		</action>
		<action name="view_person" class="viewPerson">
			<result type="tiles">ab.person.view</result>
		</action>
		<action name="setRegistrationForm" class="listApartments">
			<result type="tiles">ab.person.set_registration_form</result>
		</action>
		<action name="setRegistration" class="setRegistrationAction">
			<result name="success" type="redirect-action">
				<param name="actionName">view_person</param>
				<param name="parse">true</param>
				<param name="person.id">${personId}</param>
			</result>
			<result name="form" type="redirect-action">
				<param name="actionName">setRegistrationForm</param>
				<param name="parse">true</param>
				<param name="person.id">${personId}</param>
				<param name="apartment_not_selected">true</param>
			</result>
		</action>
	</package>

	<package name="buildings" namespace="/dicts" extends="common">
		<action name="list_buildings" class="listBuildings">
			<result type="tiles">ab.building.list</result>
		</action>
	</package>

	<package name="apartments" namespace="/dicts" extends="common">
		<action name="list_apartments" class="listApartments">
			<result type="tiles">ab.apartment.list</result>
		</action>
	</package>

</struts>
