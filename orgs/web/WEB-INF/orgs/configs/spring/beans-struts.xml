<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- Organisations -->
	<bean id="organizationsListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.OrganizationsListAction"
		  p:organizationService-ref="organizationService" />

	<bean id="organizationDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.OrganizationDeleteAction"
		  p:organizationService-ref="organizationService" />

	<bean id="organizationEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.OrganizationEditAction"
		  p:organizationService-ref="organizationService" />

	<bean id="organizationHelper" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.OrganizationHelper"
		  p:organizationService-ref="organizationService" />

	<bean id="paymentPointHelper" scope="prototype"
		  class="org.flexpay.orgs.actions.paymentpoint.PaymentPointHelper"
		  p:paymentPointService-ref="paymentPointService" />

	<!-- Bank actions -->
	<bean id="banksListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.BanksListAction"
		  p:bankService-ref="bankService" />

	<bean id="bankDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.BankDeleteAction"
		  p:bankService-ref="bankService" />

	<bean id="bankEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.BankEditAction"
		  p:organizationService-ref="organizationService"
		  p:bankService-ref="bankService" />

	<!-- Payments Collectors actions -->
	<bean id="paymentsCollectorsListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.PaymentsCollectorsListAction"
		  p:organizationService-ref="organizationService"
		  p:collectorService-ref="paymentsCollectorService"
		  p:crumbNameKey="orgs.crumbs.payments_collectors_list" />

	<bean id="paymentsCollectorsDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.PaymentsCollectorsDeleteAction"
		  p:collectorService-ref="paymentsCollectorService" />

	<bean id="paymentsCollectorEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.PaymentsCollectorEditAction"
		  p:organizationService-ref="organizationService"
		  p:instanceService-ref="paymentsCollectorService"
		  p:crumbNameKey="orgs.crumbs.payments_collector_edit" />

	<bean id="paymentsCollectorDetailsAction"
		  class="org.flexpay.orgs.actions.organization.PaymentsCollectorDetailsAction"
		  p:organizationHelper-ref="organizationHelper"
		  p:collectorService-ref="paymentsCollectorService"
		  p:paymentPointService-ref="paymentPointService"
		  p:crumbNameKey="orgs.crumbs.payments_collector_details"
          scope="prototype" />

	<!-- Service organization actions -->
	<bean id="serviceOrganizationsListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.ServiceOrganizationsListAction"
		  p:serviceOrganizationService-ref="serviceOrganizationService" />

	<bean id="serviceOrganizationDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.ServiceOrganizationDeleteAction"
		  p:serviceOrganizationService-ref="serviceOrganizationService" />

	<bean id="serviceOrganizationEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.ServiceOrganizationEditAction"
		  p:organizationService-ref="organizationService"
		  p:serviceOrganizationService-ref="serviceOrganizationService"
		  p:objectsFactory-ref="orgsObjectsFactory" />

	<!-- Service provider actions -->
	<bean id="serviceProvidersListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.serviceprovider.ServiceProvidersListAction"
		  p:providerService-ref="serviceProviderService" />

	<bean id="serviceProviderDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.serviceprovider.ServiceProviderDeleteAction"
		  p:providerService-ref="serviceProviderService" />

	<bean id="serviceProviderEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.serviceprovider.ServiceProviderEditAction"
		  p:organizationService-ref="organizationService"
		  p:providerService-ref="serviceProviderService" />

	<!-- Subdivision actions -->
	<bean id="subdivisionsListAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.SubdivisionsListAction"
		  p:organizationService-ref="organizationService"
		  p:subdivisionService-ref="subdivisionService" />

	<bean id="subdivisionDeleteAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.SubdivisionDeleteAction"
		  p:subdivisionService-ref="subdivisionService" />

	<bean id="subdivisionEditAction" scope="prototype"
		  class="org.flexpay.orgs.actions.organization.SubdivisionEditAction"
		  p:organizationService-ref="organizationService"
		  p:subdivisionService-ref="subdivisionService" />

    <!-- Payment points actions -->
    <bean id="paymentPointsListPageAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointsListPageAction"
          p:crumbNameKey="orgs.crumbs.payment_points_list"
          p:collectorService-ref="paymentsCollectorService"
          scope="prototype" />

    <bean id="paymentPointsListAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointsListAction"
          p:paymentPointService-ref="paymentPointService"
          scope="prototype" />

    <bean id="paymentPointDeleteAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointDeleteAction"
          p:paymentPointService-ref="paymentPointService"
          scope="prototype" />

    <bean id="paymentPointEditAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointEditAction"
          p:paymentPointService-ref="paymentPointService"
          p:paymentsCollectorService-ref="paymentsCollectorService"
          p:crumbNameKey="orgs.crumbs.payment_point_edit"
          p:crumbCreateKey="orgs.crumbs.payment_point_create"
          scope="prototype" />

    <bean id="paymentPointViewAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointViewAction"
          p:paymentPointService-ref="paymentPointService"
          p:crumbNameKey="orgs.crumbs.payment_point_view"
          scope="prototype" />

    <bean id="paymentPointDetailsAction"
          class="org.flexpay.orgs.actions.paymentpoint.PaymentPointDetailsAction"
          p:cashboxService-ref="cashboxService"
          p:paymentsCollectorService-ref="paymentsCollectorService"
          p:paymentPointService-ref="paymentPointService"
          p:crumbNameKey="orgs.crumbs.payment_point_details"
          scope="prototype" />

	<!-- cashboxes actions -->
    <bean id="cashboxViewAction"
          class="org.flexpay.orgs.actions.cashbox.CashboxViewAction"
          p:cashboxService-ref="cashboxService"
          p:crumbNameKey="orgs.crumbs.cashbox_view"
          scope="prototype" />

	<bean id="cashboxDeleteAction"
		  class="org.flexpay.orgs.actions.cashbox.CashboxDeleteAction"
		  p:cashboxService-ref="cashboxService"
		  scope="prototype" />

	<bean id="cashboxEditAction"
		  class="org.flexpay.orgs.actions.cashbox.CashboxEditAction"
		  p:cashboxService-ref="cashboxService"
		  p:paymentPointService-ref="paymentPointService"
		  p:crumbNameKey="orgs.crumbs.cashbox_edit"
		  p:crumbCreateKey="orgs.crumbs.cashbox_create"
		  scope="prototype" />

	<bean id="cashboxesListAction"
		  class="org.flexpay.orgs.actions.cashbox.CashboxesListAction"
		  p:cashboxService-ref="cashboxService"
		  scope="prototype" />

    <bean id="cashboxesListPageAction" parent="staticPageAction"
          p:crumbNameKey="orgs.crumbs.cashboxes_list"
          scope="prototype" />

</beans>
