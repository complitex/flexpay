<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC
		"-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN"
		"http://www.sun.com/software/dtd/appserver/sun-web-app_2_5-0.dtd">
<sun-web-app>

	<security-role-mapping>
		<role-name>ROLE_BASIC</role-name>
		<group-name>FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_COMMON_DEVELOPER</role-name>
		<group-name>developerRole</group-name>
	</security-role-mapping>


	<!-- Organizations -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_ORGANIZATION_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_ORGANIZATION_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_ORGANIZATION_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_ORGANIZATION_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- Banks -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_BANK_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_BANK_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_BANK_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_BANK_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- Service organizations -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_ORGANIZATION_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_ORGANIZATION_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_ORGANIZATION_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_ORGANIZATION_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- Service providers -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_PROVIDER_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_PROVIDER_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_PROVIDER_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_SERVICE_PROVIDER_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- Payments collectors -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_COLLECTOR_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_COLLECTOR_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_COLLECTOR_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_COLLECTOR_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- Payment points -->
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_POINT_READ</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_POINT_CHANGE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_POINT_ADD</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ROLE_ORGS_PAYMENT_POINT_DELETE</role-name>
		<group-name>ORGS_FULL_ACCESS</group-name>
	</security-role-mapping>

	<!-- cashbox roles -->
    <security-role-mapping>
        <role-name>ROLE_ORGS_CASHBOX_READ</role-name>
        <group-name>ORGS_FULL_ACCESS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ROLE_ORGS_CASHBOX_CHANGE</role-name>
        <group-name>ORGS_FULL_ACCESS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ROLE_ORGS_CASHBOX_ADD</role-name>
        <group-name>ORGS_FULL_ACCESS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ROLE_ORGS_CASHBOX_DELETE</role-name>
        <group-name>ORGS_FULL_ACCESS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ROLE_ORGS_CASHBOX_READ_ALIAS_PAYMENT_COLLECTORS</role-name>
        <group-name>ORGS_FULL_ACCESS</group-name>
    </security-role-mapping>

	<parameter-encoding default-charset="UTF-8" />

</sun-web-app>
