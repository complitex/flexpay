<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- import services definitions -->
	<import resource="beans-services.xml" />

	<!-- Define struts actions beans -->
	
	<bean id="importFileAction" class="org.flexpay.sz.actions.ImportFileAction" scope="prototype">
		<property name="regionService" ref="regionService" />
		<property name="countryService" ref="countryService" />
		<property name="importFileService" ref="importFileService" />
	</bean>
	<bean id="importFileListAction" class="org.flexpay.sz.actions.ImportFileListAction" scope="prototype">
		<property name="importFileService" ref="importFileService" />
	</bean>
	
</beans>	