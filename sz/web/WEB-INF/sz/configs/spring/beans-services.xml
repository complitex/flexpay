<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="szFileService" class="org.flexpay.sz.service.imp.SzFileServiceImpl"
        p:szFileDao-ref="szFileDAO"
        p:szFileDaoExt-ref="szFileDAOExt" />

	<bean id="osznService" class="org.flexpay.sz.service.imp.OsznServiceImpl"
		p:osznDao-ref="osznDAO" />

	<bean id="characteristicRecordService" class="org.flexpay.sz.service.imp.CharacteristicRecordServiceImpl"
		p:characteristicRecordDao-ref="characteristicRecordDAO" />

	<bean id="subsidyRecordService" class="org.flexpay.sz.service.imp.SubsidyRecordServiceImpl"
		p:subsidyRecordDao-ref="subsidyRecordDAO" />

	<bean id="serviceTypeRecordService" class="org.flexpay.sz.service.imp.ServiceTypeRecordServiceImpl"
		p:serviceTypeRecordDao-ref="serviceTypeRecordDAO" />

	<bean id="szCorrectionsService" class="org.flexpay.sz.service.imp.CorrectionsServiceImpl"
		p:apartmentCorrectionDao-ref="apartmentCorrectionDao"
		p:buildingCorrectionDao-ref="buildingCorrectionDao"
		p:districtCorrectionDao-ref="districtCorrectionDao"
		p:streetCorrectionDao-ref="streetCorrectionDao" />

    <bean parent="processManager">
        <property name="definitionPaths">
            <list>
                <value>WEB-INF/sz/process</value>
            </list>
        </property>
    </bean>

</beans>
