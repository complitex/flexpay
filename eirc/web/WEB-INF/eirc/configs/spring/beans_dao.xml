				  value="org.flexpay.eirc.dao.ServiceDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Service" />
			</bean>
		</property>
	</bean>

