<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="serviceDaoExt" class="org.flexpay.eirc.dao.imp.ServiceDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
		<property name="serviceDaoJDBC">
			<bean class="org.flexpay.eirc.dao.imp.ServiceDaoJDBC">
				<property name="dataSource" ref="dataSource" />
			</bean>
		</property>
	</bean>

	<bean id="spFileDaoExt" class="org.flexpay.eirc.dao.imp.SpFileDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="spRegistryDaoExt" class="org.flexpay.eirc.dao.imp.SpRegistryDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>


	<bean id="spFileDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpFileDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpFile" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistry" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryRecordDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryRecord" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordDaoExt"
		  class="org.flexpay.eirc.dao.imp.SpRegistryRecordDaoExtImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="spRegistryTypeDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryTypeDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryType" />
			</bean>
		</property>
	</bean>

	<bean id="consumerDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ConsumerDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Consumer" />
			</bean>
		</property>
	</bean>

	<bean id="consumerDaoExt" class="org.flexpay.eirc.dao.imp.ConsumersDaoExtJdbcImpl">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="organisationDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.OrganisationDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Organisation" />
			</bean>
		</property>
	</bean>

	<bean id="accountRecordDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.AccountRecordDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.AccountRecord" />
			</bean>
		</property>
	</bean>
	
	<bean id="serviceOrganisationDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.ServiceOrganisationDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.ServiceOrganisation" />
			</bean>
		</property>
	</bean>
	
	<bean id="ticketDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.TicketDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.Ticket" />
			</bean>
		</property>
	</bean>
	
	<bean id="ticketServiceAmountDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.TicketServiceAmountDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.TicketServiceAmount" />
			</bean>
		</property>
	</bean>
	
	<bean id="spRegistryStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryStatus" />
			</bean>
		</property>
	</bean>
	
	<bean id="spRegistryArchiveStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryArchiveStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryArchiveStatus" />
			</bean>
		</property>
	</bean>

	<bean id="spRegistryRecordStatusDao" parent="abstractDao">
		<property name="proxyInterfaces"
				  value="org.flexpay.eirc.dao.SpRegistryRecordStatusDao" />
		<property name="target">
			<bean parent="abstractDaoTarget">
				<constructor-arg value="org.flexpay.eirc.persistence.SpRegistryRecordStatus" />
			</bean>
		</property>
	</bean>

</beans>
