<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
		"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.devMode" value="true" />
	<constant name="struts.custom.i18n.resources" value="/i18n/common-massages" />

    <package name="common" namespace="/common" extends="tiles-default">

        <interceptors>
            <interceptor name="flexpayRoles" class="org.flexpay.common.actions.interceptor.FlexpayRolesInterceptor"/>
        </interceptors>

        <action name="welcome">
            <result type="tiles">common.welcome</result>
		</action>

	</package>



</struts>