<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
		"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="tc-buildingattributes" namespace="/tc" extends="common">

        <action name="buildingsListAjax" class="buildingsListAjaxAction">
            <result>/WEB-INF/jsp/tc/building_attributes/buildings_list.jsp</result>
        </action>

        <action name="buildingsList" class="staticPageAction">
            <result type="tiles">tc.building_attributes.list</result>
        </action>

		<action name="buildingAttributesEdit" class="buildingAttributesEditAction">
			<result name="input" type="tiles">tc.building_attributes.edit</result>
			<result name="success" type="tiles">tc.building_attributes.edit</result>
		</action>

	</package>

</struts>
