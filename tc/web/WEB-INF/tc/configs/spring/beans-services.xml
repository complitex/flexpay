<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
		   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <bean id="tariffService" class="org.flexpay.tc.service.impl.TariffServiceImpl"
          p:tariffDao-ref="tariffDao" />

    <bean id="tariffCalculationResultService" class="org.flexpay.tc.service.impl.TariffCalculationResultServiceImpl"
          p:tariffCalculationResultDao-ref="tariffCalculationResultDao" />

    <bean id="tariffCalculationRulesFileService" class="org.flexpay.tc.service.impl.TariffCalculationRulesFileServiceImpl"
          p:tariffCalculationRulesFileDao-ref="tariffCalculationRulesFileDao" />

</beans>
